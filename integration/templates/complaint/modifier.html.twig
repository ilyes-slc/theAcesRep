{% extends 'baseFront.html.twig' %} 




{% block content %}
<br><br><br><br>
<div class="page-wrapper bg-gra-01 p-t-180 p-b-100 font-poppins">
        <div class="wrapper wrapper--w780">
            <div class="card card-3">
                <div class="card-heading"></div>
                <div class="card-body">
                    <h2 class="title">Add Complaint</h2>
                    <form method="POST">
                        {{ form_start(form,{'attr': {'novalidate': 'novalidate'}} ) }}
                        <div class="input-group">
                            {{ form_errors(form) }}
                            {{ form_widget(form.description, {'attr': {'class': 'input--style-3'}}) }}
                        </div>
                        <div class="input-group">
                            <div class="rs-select2 js-select-simple select--no-search">
                                {{ form_widget(form.target, {'attr': {'class': 'rs-select2 js-select-simple select--no-search'}}) }}
                                <div class="select-dropdown"></div>
                            </div>
                        </div>
                        <div class="input-group">
                            <div class="rs-select2 js-select-simple select--no-search">
                                {{ form_widget(form.method_remb, {'attr': {'class': 'rs-select2 js-select-simple select--no-search'}}) }}
                                <div class="select-dropdown"></div>
                            </div>
                        </div>
                        <div class="p-t-10">
                            {{ form_widget(form.save) }}
                        </div>
                        {{ form_end(form) }}
                        {{ form_rest(form) }}
                    </form>
                </div>
            </div>
        </div>
    </div>
<br><br><br><br>
{% endblock %}